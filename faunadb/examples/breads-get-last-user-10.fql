Map(
  Paginate(
    Join(
      Match(Index("breads_by_user_nano_id"), "__user_nano_id__"),
      Index("breads_sort_by_ts_desc_with_ref")
    ),
    {
      size: 10
    }
  ),
  Lambda(
    ["ts", "ref"],
    {
      nanoId: Select(["data", "nanoId"], Get(Var("ref"))),
      title: Select(["data", "title"], Get(Var("ref"))),
      userId: Select(["data", "userId"], Get(Var("ref"))),
    }
  )
)
