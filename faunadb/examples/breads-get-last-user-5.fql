Map(
  Map(
    Paginate(
      Join(
        Match(Index("breads_by_user_nano_id"), "__user_nano_id__"),
        Index("breads_sort_by_ts_desc_with_ref")
      ),
      {
        size: 5
      }
    ),
    Lambda(
      ["ts", "ref"],
      Get(Var("ref"))
    )
  ),
  Lambda(
    "item",
    {
      nanoId: Select(["data", "nanoId"], Var("item")),
      title: Select(["data", "title"], Var("item")),
      userId: Select(["data", "userId"], Var("item")),
    }
  )
)
